<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.innovhr.innovhrapp.controllers.ContinueAsController"
      alignment="CENTER"
      spacing="10.0">

    <Button fx:id="adminButton" text="Admin" onAction="#handleAdminSelection" visible="false"/>
    <Button fx:id="managerButton" text="Manager" onAction="#handleManagerSelection" visible="false"/>
    <Button fx:id="collabButton" text="Collaborator" onAction="#handleCollabSelection" visible="false"/>

</VBox>
